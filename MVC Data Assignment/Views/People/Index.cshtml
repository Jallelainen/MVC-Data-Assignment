@model List<People>
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<a asp-action="Create">Add Person to list</a>

<form asp-action="Index" method="post">
    <label asp-for="Search"></label>
    <input asp-for="Name" />
    <span asp-validation-for="Name"></span>
</form>

<table>
    <tr>
        <td>Name</td>
        <td>Phone</td>
        <td>City</td>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.PhoneNum</td>
            <td>@item.City</td>
        </tr>
    }
</table>